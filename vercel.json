{
  "version": 2,
  "builds": [
    {
      "src": "omans-site/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "omans-site/build" }
    },
    {
      "src": "server/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/omans-site/$1"
    }
  ]
}
